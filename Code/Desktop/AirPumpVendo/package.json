{
  "name": "airpumpvendo",
  "version": "1.0.0",
  "description": "",
  "main": "electron/main.js",
  "scripts": {
    "dev": "concurrently -k -n NEXT,ELECTRON -c auto \"npm:dev:ui\" \"npm:dev:electron\"",
    "dev:ui": "cd ui && next dev -p 3000",
    "dev:electron": "wait-on tcp:3000 && cross-env NODE_ENV=development DEV_SERVER_URL=http://localhost:3000 electron electron/main.js",
    "build:ui": "cd ui && next build && next export",
    "start": "cross-env NODE_ENV=production electron electron/main.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
    "electron": "^39.0.0",
    "electron-builder": "^26.0.12",
    "rimraf": "^6.0.1",
    "wait-on": "^9.0.1"
  },
  "dev:ui": "cd ui && next dev",
  "build:ui": "cd ui && next build && next export",
  "clean": "rimraf ui/out",
  "build:app:arm64": "electron-builder --linux=AppImage --arm64",
  "build:app:armv7l": "electron-builder --linux=AppImage --armv7l"
}
